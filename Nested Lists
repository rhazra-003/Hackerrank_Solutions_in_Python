marksheet=[]
scorelist=[]

if __name__ == '__main__':

        for _ in range(int(input())):
                name = input()
                score = float(input())
                marksheet+=[[name,score]]
                scorelist+=[score]

        score_max = sorted(list(set(scorelist)))[1] 
        
        for n,s in sorted(marksheet):
             # if the 
             if s == score_max:
                    print(n)
